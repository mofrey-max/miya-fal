                name: "song.db",
                location: "default"
            }).then((db) => {
                db.executeSql(query, [])
                    .then((res) => {
                    const song = [];
                    if (res.rows.length > 0) {
                        song.push(res.rows.item(0));
                    }
                    resolve(song);
                }, (error) => {
                    reject(error);
                });
            });
        });
    }
    filterSearch(searchTerm) {
        return this.song.filter(song => {
            return song.id.toLowerCase().indexOf(searchTerm.toLowerCase()) > -1
